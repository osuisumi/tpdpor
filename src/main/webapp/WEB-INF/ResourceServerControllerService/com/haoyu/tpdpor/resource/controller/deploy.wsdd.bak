<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from ResourceServerControllerService WSDL service -->

  <service name="ResourceServerController" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://controller.resource.tpdpor.haoyu.com"/>
      <parameter name="wsdlServiceElement" value="ResourceServerControllerService"/>
      <parameter name="schemaQualified" value="http://controller.resource.tpdpor.haoyu.com,http://xml.apache.org/xml-soap,http://web.core.sip.haoyu.com"/>
      <parameter name="wsdlServicePort" value="ResourceServerController"/>
      <parameter name="className" value="com.haoyu.tpdpor.resource.controller.ResourceServerControllerSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="ResourceServerController"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="entities" qname="operNS:entities" xmlns:operNS="http://controller.resource.tpdpor.haoyu.com" returnQName="retNS:entitiesReturn" xmlns:retNS="http://controller.resource.tpdpor.haoyu.com" returnType="rtns:ArrayOf_xsd_anyType" xmlns:rtns="http://controller.resource.tpdpor.haoyu.com" returnItemQName="tns:item" xmlns:tns="http://controller.resource.tpdpor.haoyu.com" soapAction="" >
        <parameter qname="pns:searchParam" xmlns:pns="http://controller.resource.tpdpor.haoyu.com" type="tns:SearchParam" xmlns:tns="http://web.core.sip.haoyu.com"/>
      </operation>
      <parameter name="allowedMethods" value="entities"/>

      <typeMapping
        xmlns:ns="http://web.core.sip.haoyu.com"
        qname="ns:SearchParam"
        type="java:com.haoyu.sip.core.web.SearchParam"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
      <typeMapping
        xmlns:ns="http://controller.resource.tpdpor.haoyu.com"
        qname="ns:>entities"
        type="java:com.haoyu.tpdpor.resource.controller.Entities"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
      <typeMapping
        xmlns:ns="http://controller.resource.tpdpor.haoyu.com"
        qname="ns:>entitiesResponse"
        type="java:com.haoyu.tpdpor.resource.controller.EntitiesResponse"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
      <arrayMapping
        xmlns:ns="http://controller.resource.tpdpor.haoyu.com"
        qname="ns:ArrayOf_xsd_anyType"
        type="java:java.lang.Object[]"
        innerType="cmp-ns:anyType" xmlns:cmp-ns="http://www.w3.org/2001/XMLSchema"
        encodingStyle=""
      />
      <typeMapping
        xmlns:ns="http://xml.apache.org/xml-soap"
        qname="ns:mapItem"
        type="java:org.apache.xml.xml_soap.MapItem"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
  </service>
</deployment>
